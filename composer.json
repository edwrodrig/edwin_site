{
    "name": "edwrodrig/mypage",
    "description" : "Mi página personal",
    "authors": [
        {
            "name": "Edwin Rodríguez-León",
            "homepage": "http://www.edwin.cl",
            "role": "Developer"
        }
    ],
    "minimum-stability" : "stable",
    "require": {
        "php" : "^7.4",
        "labo86/staty" : "0.3.0",
        "labo86/hapi" : "^0.1",
        "edwrodrig/image" : "^3.0",
        "edwrodrig/deployer" : "^2.1",
        "ext-json": "*",
        "ext-imagick": "*"
    },
    "require-dev" : {
        "phpunit/phpunit" : "^8.5"
    },
    "autoload" : {
        "psr-4" : {
            "edwrodrig\\mypage\\model\\" : "modules/model/src/",
            "edwrodrig\\mypage\\site\\" : "modules/site/src/",
            "edwrodrig\\mypage\\ws\\" : "modules/ws/src/"
        }
    },
    "autoload-dev" : {
        "psr-4" : {
            "test\\edwrodrig\\mypage\\model\\" : "modules/model/test/",
            "test\\edwrodrig\\mypage\\site\\" : "modules/site/test/",
            "test\\edwrodrig\\mypage\\ws\\" : "modules/ws/test/"
        }
    }
}
